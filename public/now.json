{
  "version": 2,
  "name": "band-bridge",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/manifest.json",
        "dest": "/manifest.json"
      }, 
      {
        "src": "/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}